<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE task PUBLIC "-//OASIS//DTD DITA Task//EN" "task.dtd">
<task id="task_view-datasets">
    <title>View Defined Datasets</title>
    <shortdesc>View the list of defined datasets on the <wintitle>Manage Datasets</wintitle>
        page.</shortdesc>
    <taskbody>
        <context>
            <p>To see what datasets have already been defined:</p>
        </context>
        <steps>
            <step>
                <cmd>Open the <uicontrol>Manage Datasets</uicontrol> page, by clicking the
                        <uicontrol>Datasets</uicontrol> icon in the left navigation bar. <image
                        href="../_images/gui-screen-shots/icon_datasets.jpg" id="image_fmr_wzn_mq"
                        scale="50"/></cmd>
                <info>
                    <p>The <wintitle>Manage Datasets</wintitle> page opens, listing the datasets
                        that have already been defined.</p>
                    <fig id="fig_manage-datasets">
                        <image href="../_images/gui-screen-shots/datasets_view.png"
                            id="image_j4l_xkn_jq" scale="60"/>
                    </fig>
                </info>
            </step>
        </steps>
    </taskbody>
</task>
