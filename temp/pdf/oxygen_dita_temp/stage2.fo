        <fo:list-block space-before="0.6em" space-after="0.6em" provisional-distance-between-starts="5mm" provisional-label-separation="1mm">
            <fo:list-item space-after="3pt" space-before="3pt"><fo:list-item-label keep-together.within-line="always" keep-with-next.within-line="always" end-indent="label-end()"><fo:block text-align="start" font-weight="bold"><fo:inline/>1. </fo:block></fo:list-item-label><fo:list-item-body start-indent="body-start()"><fo:block>
                <fo:block>In the address bar of the browser, enter the URL configured for the <fo:inline border-left-width="0pt" border-right-width="0pt">PHEMI Central</fo:inline>
                    <fo:inline border-left-width="0pt" border-right-width="0pt">Management and Governance Console</fo:inline>.</fo:block>
                <fo:block space-before="3pt" space-after="3pt"/>
                <fo:block>
                    <fo:block space-before="0.6em" space-after="0.6em" text-indent="0em">The <fo:inline border-left-width="0pt" border-right-width="0pt">PHEMI Central</fo:inline> login screen appears.</fo:block>
                    <fo:block id="unique_31_Connect_42_fig_login-screen"><fo:inline id="unique_31_Connect_42_image_kym_blr_3q"><fo:external-graphic src="url(file:/C:/PHEMI-docs/docs/_common_source/_images/gui-screen-shots/login-screen.JPG)"/></fo:inline></fo:block>
